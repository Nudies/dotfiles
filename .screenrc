# 256 color
attrcolor b ".I"
termcapinfo xterm 'Co#256:AB=\E[48;5;%dm:AF=\E[38;5;%dm'
defbce "on"
vbell off
startup_message off
defscrollback 5000
msgwait 1.5
shelltitle "$ |"
caption always '%{= kw}%?%-Lw%?%{g}(%{g}%n*%f%t%?(%u)%?%{g})%{w}%?%+Lw%?%?'
hardstatus string "screen(%n): %h"
termcapinfo xterm ti@:te@
altscreen on
escape ^Jj